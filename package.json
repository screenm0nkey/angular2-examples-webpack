{
  "name": "angular2-examples-webpack",
  "version": "1.0.0",
  "description": "A collection of Angular2 examples",
  "scripts": {
    "typings-install": "./node_modules/.bin/typings install",
    "postinstall": "npm run typings-install",
    "build": "./node_modules/.bin/webpack --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "start": "./node_modules/.bin/webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src"
  },
  "license": "MIT",
  "devDependencies": {
    "body-parser": "^1.15.0",
    "es6-promise": "3.0.2",
    "es6-shim": "0.35.0",
    "express": "^4.13.4",
    "express-cors": "0.0.3",
    "reflect-metadata": "0.1.2",

    "awesome-typescript-loader": "^0.17.0",
    "url-loader": "^0.5.7",
    "style-loader": "^0.13.1",
    "raw-loader": "0.5.1",
    "source-map-loader": "^0.1.5",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "es6-promise-loader": "^1.0.1",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "to-string-loader": "^1.1.4",
    "html-webpack-plugin": "^2.21.0",
    "copy-webpack-plugin": "^3.0.1",

    "ts-helpers": "1.1.1",
    "ts-node": "^0.9.1",
    "typedoc": "^0.4.3",
    "typescript": "~1.8.10",
    "typings": "~1.3.0",

    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "@angular/http": "2.0.0-rc.2",
    "@angular/common": "2.0.0-rc.2",
    "@angular/compiler": "2.0.0-rc.2",
    "@angular/core": "2.0.0-rc.2",
    "@angular/forms": "^0.1.0",
    "@angular/platform-browser": "2.0.0-rc.2",
    "@angular/platform-browser-dynamic": "2.0.0-rc.2",
    "@angular/platform-server": "2.0.0-rc.2",
    "@angular/router": "2.0.0-rc.2",
    "@angular/router-deprecated": "2.0.0-rc.2",

    "ie-shim": "^0.1.0",
