{
  "name": "angular2-examples-webpack",
  "version": "1.0.0",
  "description": "A collection of Angular2 examples",
  "scripts": {
    "typings-install": "./node_modules/.bin/typings install",
    "postinstall": "npm run typings-install",
    "build": "./node_modules/.bin/webpack --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "start": "./node_modules/.bin/webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src"
  },
  "license": "MIT",
  "devDependencies": {
    "awesome-typescript-loader": "~0.16.2",
    "body-parser": "^1.15.0",
    "es6-promise": "3.0.2",
    "es6-shim": "0.35.0",
    "express": "^4.13.4",
    "express-cors": "0.0.3",
    "reflect-metadata": "0.1.2",
    "source-map-loader": "^0.1.5",
    "typescript": "~1.8.9",
    "typings": "^0.7.9",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4"
  },
  "dependencies": {
    "@ngrx/store": "^1.3.3",
    "angular2": "2.0.0-beta.13",
    "core-js": "^2.2.0",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "~0.6.6"
  },
  "keywords": [
    "Angular2"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/screenm0nkey/angular2-examples-webpack.git"
  },
  "bugs": {
    "url": "https://github.com/screenm0nkey/angular2-examples-webpack/issues"
  },
  "homepage": "https://github.com/screenm0nkey/angular2-examples-webpack#readme"
}
